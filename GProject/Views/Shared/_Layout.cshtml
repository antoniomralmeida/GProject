<!DOCTYPE html>
<html lang="pt">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - Meu aplicativo ASP.NET</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <script src="https://code.jquery.com/jquery-1.10.2.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")

    
</head>
<body>

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("GProjet", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                   <!-- <li>@Html.ActionLink("Início", "Index", "Home", new { area = "" }, null)</li> -->
                    
                  
                        
                    <li>
                        <a class="navbar-collapse collapse" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Menu
                            <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            <li>@Html.ActionLink("Aprova Finalização de Atividade", "ApproveFinalizationActivities", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Atribuir atividades", "AssignActivities", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Cadastra Gerente Projeto", "CadGerente", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Criar projeto", "CreateProject", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Finalizar atividade", "EndActivity", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Gerenciar Atividades", "ManageActivities", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Executar atividades", "PerformActivities", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Informa impedimento de atividade", "ReportsActivityImpediment", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Informa duração da atividade", "ReportsDurationActivity", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Selecionar membro para projeto", "SelectMemberProject", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Acompanhar atividades", "TrackActivities", "Home", new { area = "" }, null)</li>
                            <li>@Html.ActionLink("Visualizar projeto", "ViewProject", "Home", new { area = "" }, null)</li>
                        </ul>


                    </li>
                </ul>

                
                @Html.Partial("_LoginPartial")

               
            </div>

        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        @RenderSection("SPAViews", required: false)
        <hr />
        <footer>
            <p>&copy; GProjet LTDA</p>
        </footer>
    </div>

    
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>

    <script>
    $( "#casa" ).click(function() {
      $( this ).slideUp();
    });
    </script>

</html>
